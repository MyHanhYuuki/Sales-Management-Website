</head>

<body class="page-session page-sound page-header-fixed page-sidebar-fixed demo-dashboard-session">

    @if ((Session["Account"] == null) &&
         (HttpContext.Current.Request.Url.AbsolutePath == "/" || HttpContext.Current.Request.Url.AbsolutePath.Contains("/Home")))
    {
        // Render if not login, login faile, redirect to Homepage
        <section class="background"></section>
        @RenderBody()

    }

    else if (Session["Account"] != null)
    {
        // Render section menu and header if login successfull
        <section id="wrapper">
            @Html.Partial("~/Areas/Admin/Views/PartitalView/HeaderTopManagerPartial.cshtml")

        </section>
        <section id="wrapper">
            @Html.Action("GetMenu", "Home", new { data = Model })
        </section>

        <div id="wrapper">
            <div id="page-wrapper" style="background-color: rgb(242, 244, 248);">
                <div class="graphs">
                    @if (TempData["AlertMessage"] != null && (string)TempData["AlertMessage"] != "")
                    {
                        <div id="AlertBox" class="alert @TempData["AlertType"] hide">@Html.Raw(TempData["AlertMessage"]) </div>
                    }
                    @RenderBody()
                </div>
            </div>
        </div>

        <div id="back-top" class="animated pulse circle">
            <i class="fa fa-angle-up"></i>
        </div>

        <script src="~/Scripts/alert.js"></script>
    }
    else
    {
        // Redirect to error page if access to page which you dont have permission to access
        <script language="javascript">
            window.location.replace("/Admin/Home/Index");
        </script>
        @RenderBody()
    }

